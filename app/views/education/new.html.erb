<turbo-frame id="new_education">
  <%= form_with(model: @education, url: education_index_path) do |form| %>
    <%= form.text_field :title, placeholder:"台灣大學-電機系", class:"input dark:bg-gray-800 dark:text-gray-300"%>
    <%= form.date_field :start_date, class:"input dark:bg-gray-800 dark:text-gray-300" %>
    <%= form.date_field :end_date, class:"input dark:bg-gray-800 dark:text-gray-300" %>
    <%= form.submit '新增', class:"save-btn"%>
  <% end %>
</turbo-frame>
